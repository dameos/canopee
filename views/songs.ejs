<html> 
    <head> 
        <% include partials/head %>
    </head>
    <title>Song</title>
    <body>
        <div class="container">
            <header>
                <%include partials/header %>
            </header>

            <main>
                <div class="row">
                    <div class="col-sm-8">
                        <div class="jumbotron">
                            <h1>List of Songs</h1>
                                <ul>
                                    <% songs.forEach(function(song) { %>
                                        <li>
                                        <img src= <%= song.songImageUrl %> alt='' height="150" width="150">
                                        <%= song.songName %> - <%= song.songAuthor %> - <%= song.songDuration %> 
                                        </li>
                                    <% }); %>
                                </ul>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </body>
</html>